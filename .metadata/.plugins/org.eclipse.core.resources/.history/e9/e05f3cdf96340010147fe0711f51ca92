package questions;

public class TwoDigitElementSearch 
{
	public static void main(String[] args) 
	{
		
		
		int[] arr = {22, 309, 4, 55};
		
		for(int i=0; i<arr.length; i++)
		{
			int count = 0 ;
			while(arr[i]>0)
			{
				arr[i]/=10;
				count++;
			}
			System.out.println(arr[i] + "is a " + count +"digit number");
		
		}
	}

}
