package questions;

public class Occurence 
{

	public static void main(String[] args) 
	{
		int[] arr = {1, 5, 2, 5, 2, 2, 1, 6};
		
		for(int i=0; i<arr.length; i++)
		{
			int count = 0;
			for(int j=0; j<arr.length; j++)
			{
				if(arr[i] == arr[j])
				{
					count++;
				}
			}
			System.out.println("Occurence of "+arr[i]+"is "+count);
		}

	}

}
