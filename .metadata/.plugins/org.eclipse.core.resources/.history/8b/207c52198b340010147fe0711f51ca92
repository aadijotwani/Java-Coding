package questions;

public class Maximun 
{
	static int[] arr = {4,5,3,2,9};
	public static int mincount = arr[0],maxcount = arr[0];
	public static void main(String[] args) 
	{
		for(int i=0; i<arr.length; i++)
		{
			if(arr[i] >= maxcount)
			{
				maxcount = arr[i];
			}
			if(arr[i] <= mincount)
			{
				mincount = arr[i];
			}
				
		}
		System.out.println(maxcount);
	}

}
