package questions;

import java.util.concurrent.CountDownLatch;

public class UniqueElements 
{

	public static void main(String[] args) 
	{
		int[] arr = {10, 20, 20, 30, 40, 40, 40};
		for(int i=0; i<arr.length; i++)
		{
			for(int j = i ; j>0 ; j--)
			{
				if(arr[j] != arr[j-1])
				{
					System.out.println(arr[i]);
				}
				else 
				{
					break;
				}
				
			}
			
		}
		
	}
}
